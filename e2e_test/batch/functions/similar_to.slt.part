statement ok
create table t (id int, pat varchar, text varchar);

# example data from https://www.postgresql.org/docs/16/functions-matching.html#FUNCTIONS-SIMILARTO-REGEXP
statement ok
insert into
    t
values
    (1, 'abc', 'abc'),
    (2, 'a', 'abc'),
    (3, '%(b|d)%', 'abc'),
    (4, '(b|c)%', 'abc'),
    (5, '%abc-%', '-abc-'),
    (6, '%abc-%', 'xabcy');

