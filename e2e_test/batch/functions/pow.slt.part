query TRR
with t(v) as (
	values ('nan'), ('inf'), ('-inf'), ('-0')
) select
	v,
	exp(v::decimal)::float8, -- display with trailing zeros, use `trim_scale` later
	exp(v::float8)
from t;
----
nan       NaN      NaN
inf  Infinity Infinity
-inf        0        0
-0          1        1

query TRR
with t(v) as (
	values ('nan'), ('inf')
) select
	v,
	ln(v::decimal),
	ln(v::float8),
	log(v::decimal),
	log(v::float8),
	log10(v::decimal),
	log10(v::float8)
from t;
----
nan      NaN      NaN      NaN      NaN      NaN      NaN
inf Infinity Infinity Infinity Infinity Infinity Infinity

