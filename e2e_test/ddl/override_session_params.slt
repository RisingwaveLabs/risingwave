# When setting session params,
# certain params may override others,
# We test that behaviour in this file.

################## Test initial defaults

query I
SHOW STREAMING_RATE_LIMIT;
----
1

query B
SHOW ENABLE_STREAMING_RATE_LIMIT;
----
false

################## Test override

statement ok
SET STREAMING_RATE_LIMIT=2;

query I
SHOW STREAMING_RATE_LIMIT;
----
2

query B
SHOW ENABLE_STREAMING_RATE_LIMIT;
----
true

################## Test no override

statement ok
SET ENABLE_STREAMING_RATE_LIMIT=false;

query I
SHOW STREAMING_RATE_LIMIT;
----
2

query B
SHOW ENABLE_STREAMING_RATE_LIMIT;
----
false
