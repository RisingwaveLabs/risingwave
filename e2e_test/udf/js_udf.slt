statement ok
create function gcd(int, int) returns int language javascript as $$
    export function gcd(a, b) {
        if(a == null || b == null) {
            return null;
        }
        while (b != 0) {
            let t = b;
            b = a % b;
            a = t;
        }
        return a;
    }
$$;

