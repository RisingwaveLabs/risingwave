CREATE MATERIALIZED SOURCE tpcc_customer (
    c_id INTEGER,
    c_d_id INTEGER,
    c_w_id INTEGER,
    c_first VARCHAR,
    c_middle VARCHAR,
    c_last VARCHAR,
    c_street_1 VARCHAR,
    c_street_2 VARCHAR,
    c_city VARCHAR,
    c_state VARCHAR,
    c_zip VARCHAR,
    c_phone VARCHAR,
    c_since TIMESTAMP,
    c_credit VARCHAR,
    c_credit_lim REAL,
    c_discount REAL,
    c_balance REAL,
    c_ytd_payment REAL,
    c_payment_cnt INTEGER,
    c_delivery_cnt INTEGER,
    c_data1 VARCHAR,
    c_data2 VARCHAR,
    PRIMARY KEY (c_id, c_d_id, c_w_id)
) with (
    connector = 'kafka',
    topic = 'tpcc_customer',
    properties.bootstrap.server = '127.0.0.1:29092',
    scan.startup.mode = 'earliest'
) ROW FORMAT DEBEZIUM_JSON;

