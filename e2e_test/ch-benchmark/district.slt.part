CREATE MATERIALIZED SOURCE district (
    d_id INTEGER,
    d_w_id INTEGER,
    d_name VARCHAR,
    d_street_1 VARCHAR,
    d_street_2 VARCHAR,
    d_city VARCHAR,
    d_state VARCHAR,
    d_zip VARCHAR,
    d_tax REAL,
    d_ytd REAL,
    d_next_o_id INTEGER,
    PRIMARY KEY (d_id, d_w_id)
) with (
    connector = 'kafka',
    topic = 'postgres.public.district',
    properties.bootstrap.server = '127.0.0.1:29092',
    scan.startup.mode = 'earliest'
) ROW FORMAT DEBEZIUM_JSON;
